{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "loose": true,
        "useBuiltIns": false,
        "targets": {
          "browsers": [
            "last 4 versions",
            "ie >= 9",
            "iOS >= 7",
            "Android >= 4"
          ]
        }
      }
    ],
    "stage-0"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-runtime", 
    [
      "component", 
      [
        {
          "libraryName": "element-ui",
          "styleLibraryName": "theme-chalk"
        }
      ]
    ]
  ]
}
